Region: "hh_2l1tau_BDT"
  Type: SIGNAL
  %Label: 2\ell\mathrm{+}1\uptau_\mathrm{had}\,\,\mathrm{SR}
  Label: "2lSS1tau, SR"
  Variable: "BDTOutput_2l1tau", 10, -1., 1.
  VariableTitle: "BDT output"
  Selection: XXX_2L1TAU_SELECTION && BDTOutput_2l1tau < -0.2
  MCweight: XXX_2L1TAU_MCWEIGHT

Region: "hh_2l1tau_tauPt_1p"
  Type: SIGNAL
  Label: "2lSS1tau, SR"
  Variable: "tau_pt_0/1000", 40, 0, 200
  VariableTitle: "tau0 Pt/GeV"
  Selection: XXX_2L1TAU_SELECTION && tau_numTrack_0==1
  MCweight: XXX_2L1TAU_MCWEIGHT

Region: "hh_2l1tau_tauPt_3p"
  Type: SIGNAL
  Label: "2lSS1tau, SR"
  Variable: "tau_pt_0/1000", 40, 0, 200
  VariableTitle: "tau0 Pt/GeV"
  Selection: XXX_2L1TAU_SELECTION && tau_numTrack_0==3
  MCweight: XXX_2L1TAU_MCWEIGHT


  Selection: XXX_2L1TAUSFCR_SELECTION && XXX_2lOSSF_ZVeto && nJets_OR_DL1r_77==0 && nJets_OR>=2
  MCweight: XXX_2L1TAUSFCR_MCWEIGHT


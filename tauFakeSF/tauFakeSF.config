% HH multilepton fit
% Babar Ali
% babar.ali@cern.ch

% --------------- %
% ---  JOB    --- %
% --------------- %

Job: jobtauFakeSF
  %Label: HHML~SR
  Label: HHML
  %TexLabel: HHML~SR
  AtlasLabel: Preliminary
  CmeLabel: 13 TeV
  POI: mu_XS_hh
  InputFolder: tauFakeSF/
  %InputName: HHML
  ReadFrom: NTUP
  %%%NtuplePath: "/publicfs/atlas/atlasnew/higgs/HHML/Slim_sys_prod/SlimSysNtups_Output/MergeNtuples/July_SYS_inclusive_r1/"
  %%%NtuplePath: "/publicfs/atlas/atlasnew/higgs/hh2X/huirun/multilepton/leptau/MLntuple/MVA"
  %%%NtuplePath: "/publicfs/atlas/atlasnew/higgs/hh2X/huirun/multilepton/leptau/MLntuple/custom"
  %%%NtuplePath: "/publicfs/atlas/atlasnew/higgs/HHML/Slim_sys_prod/SlimSysNtups_Output/2LSS1Tau/tauSFCR/"
  NtuplePath: "/publicfs/atlas/atlasnew/higgs/HHML/Slim_sys_prod/SlimSysNtups_Output/2LSS1Tau/nominal/"
  NtupleName: "leptau"
  LumiLabel: 139 fb^{-1}
  %Lumi: XXX_LUMI % in pb^-1 %LUMI IS ALREADY APPLIED 
  PlotOptions: PREFITONPOSTFIT,NOXERR,YIELDS,NORMSIG %,CHI2
  DebugLevel: 2
  HistoChecks: NOCRASH
  SplitHistoFiles: TRUE
  MergeUnderOverFlow: TRUE
  ImageFormat: png,eps,pdf,%C
  MCstatThreshold: 0.01
  %%%MCstatThreshold: NONE
  MCstatConstraint: Poisson
  GetChi2: TRUE 
  %%%StatOnly: TRUE
  SystControlPlots: TRUE
  SystErrorBars: TRUE
  SystPruningShape: 0.01
  SystPruningNorm: 0.005
  %SystLarge: 1
  CorrelationThreshold: 0.05
  SystCategoryTables: TRUE
  RankingMaxNP: 15
  DoSummaryPlot: TRUE  
  SummaryPlotYmin: 0.05 
  DoTables: TRUE
  DoPieChartPlot: TRUE
  ReplacementFile: tauFakeSF/replacement.txt

% --------------- %
% ----- FIT ----- %
% --------------- %

Fit: fit
  FitType: SPLUSB
  FitRegion: CRSR
  FitBlind: TRUE
  POIAsimov: 1
  UseMinos: "mu_XS_hh"
  %doLHscan: mu_XS_hh
  %NumCPU: 30
  %GetGoodnessOfFit: TRUE	

% --------------- %
% ---- LIMIT ---- %
% --------------- %

Limit: limit
  LimitType: ASYMPTOTIC
  LimitBlind: TRUE
  %SignalInjection: TRUE
  %SignalInjectionValue: XXX

% --------------- %
% POI FLOAT NORMS %
% --------------- %

NormFactor: mu_XS_hh
  Title: "#mu (hh)"
  Min: XXX_MU_XS_HH_MIN
  Max: XXX_MU_XS_HH_MAX
  Nominal: 1
  Samples: HH
  Constant: FALSE

